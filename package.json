{
  "private": true,
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean",
    "dev": "turbo dev --no-cache  --continue --ignore='apps/admin/*'",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "test": "turbo test",
    "storybook:dev": "turbo run storybook:dev --parallel",
    "storybook:build": "turbo run storybook:build --parallel",
    "storybook:move": "turbo run storybook:move --filter=docs && turbo run storybook:move --filter=!docs"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "turbo": "^1.9.3"
  },
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "libs/*",
    "config/*"
  ]
}
